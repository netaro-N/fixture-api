        option(value="Roma")
        option(value="SD Eibar")
        option(value="SD Huesca")
        option(value="Sevilla FC")
        option(value="Valencia CF")
        option(value="Villarreal CF")
    div.form-group
      label(for="awayTeam") awayTeam
      input(type="text" name="awayTeam" autocomplete="on" list="AWAY")#awayTeam
      datalist(id="AWAY")
