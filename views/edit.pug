extends layout

block content
  h3= title
  form(method="post", action="/edit")
    div.form-group
      label(for="fixtureId") fixtureId
      input(type="text" name="fixtureId" autocomplete="on" list="ID")#fixtureId
      datalist(id="ID")
        option(value="Lali")
        option(value="Copa")
        option(value="CL")
    div.form-group
      label(for="fixtureDate") fixtureDate
      input(type="datetime-local" name="fixtureDate")#fixtureDate
    div.form-group
      label(for="fixtureSort") fixtureSort
      input(type="text" name="fixtureSort" autocomplete="on" list="SORT")#fixtureSort
      datalist(id="SORT")
        option(value="ラ・リーガ第")
        option(value="チャンピオンズリーグ")
        option(value="国王杯")
    div.form-group
      label(for="homeTeam") homeTeam
      input(type="text" name="homeTeam" autocomplete="on" list="HORM")#homeTeam
      datalist(id="HORM")
        option(value="Ajax")
